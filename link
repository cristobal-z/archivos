SELECT j.idfaltas folio, a.id_alu matricula, a.nom nombre, j.fec_ fecha,
e.nom_esp especialidad, t.nom tutor,a.sem semestre, j.obs observaciones
FROM escolar.faltas j, escolar.alu a, escolar.especialidad e, escolar.tutores t
where j.id_alu = a.id_alu and j.id_alu = e.id_alu
and j.id_alu = t.id_alu 



